{
  "builds": [
    {
      "src": "main:app",  // Replace 'main:app' with your FastAPI entrypoint file
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main:app"
    }
  ],
  "static": {
    "includeFiles": [
      "q-vercel-python.json" // Ensure this file is deployed
    ]
  }
}
